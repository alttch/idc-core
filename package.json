{
  "name": "idc-core",
  "version": "0.0.17",
  "type": "module",
  "description": "Interactive dashboard creator core",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc src/*.mts --skipLibCheck --allowImportingTsExtensions --esModuleInterop --jsx react-jsx --declaration --lib es2020,dom --downlevelIteration --emitDeclarationOnly --declarationDir dist/ && mv dist/lib.d.mts dist/idc-core.es.d.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "main": "./dist/idc-core.umd.js",
  "module": "./dist/idc-core.es.js",
  "files": [
    "dist"
  ],
  "types": "./dist/idc-core.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/idc-core.es.js",
      "require": "./dist/idc-core.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alttch/idc-core"
  },
  "author": "Bohemia Automation",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@eva-ics/webengine": "^0.6.0",
    "@eva-ics/webengine-react": "^0.2.14",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "mui-color-input": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "bmat": "^0.1.15"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
